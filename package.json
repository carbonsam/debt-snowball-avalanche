    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svelte": "^3.23.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.7.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
